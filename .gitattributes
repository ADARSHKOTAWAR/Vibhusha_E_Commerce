# Enforce consistent line endings
* text=auto

# Explicit line endings
*.js     text eol=lf
*.jsx    text eol=lf
*.ts     text eol=lf
*.tsx    text eol=lf
*.css    text eol=lf
*.html   text eol=lf
*.json   text eol=lf
*.md     text eol=lf
*.sh     text eol=lf
*.yml    text eol=lf
*.yaml   text eol=lf
*.env    text eol=lf
